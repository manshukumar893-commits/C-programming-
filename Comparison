#include <stdio.h>

int main()
{
    int a, b, c;

    printf("Enter the value of a, b & c: ");
    scanf("%d%d%d", &a, &b, &c);

    if (a > b && a > c)
    {
        printf("a is greatest: %d\n", a);
    }
    else if (b > a && b > c)
    {
        printf("b is greatest: %d\n", b);
    }
    else if (c > a && c > b)
    {
        printf("c is greatest: %d\n", c);
    }
    else
    {
        printf("Some values are equal.\n");
    }

    return 0;
}
